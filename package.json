    "copy-webpack-plugin": "^4.0.0",
    "webpack": "2.2.0"
  },
  "dependencies": {
    "electron-storage": "^1.0.7",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
